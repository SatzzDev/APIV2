<div class="main-panel">
<div class="content-wrapper">
<div class="row">
<div class="col-md-12 grid-margin">
<div class="row">
<div class="col-12 col-xl-8 mb-4 mb-xl-0">
<h3 class="font-weight-bold">REST API Dashboard</h3>
<h6 class="font-weight-normal mb-0">Status: <span class="text-success"><%= status %></span> | Total Endpoints: <span class="text-primary"><%= endpoint_total %></span></h6>
</div>
</div>
</div>
</div>
<div class="row mb-4">
<div class="col-md-3 stretch-card transparent mb-3">
<div class="card card-tale">
<div class="card-body">
<p class="mb-4">Runtime (Hours)</p>
<p class="fs-30 mb-2"><%= runtime.hours %></p>
<p><%= runtime.uptime %> uptime</p>
</div>
</div>
</div>
<div class="col-md-3 stretch-card transparent mb-3">
<div class="card card-dark-blue">
<div class="card-body">
<p class="mb-4">CPU Usage</p>
<p class="fs-30 mb-2"><%= system.cpu %>%</p>
<p>Live stat</p>
</div>
</div>
</div>
<div class="col-md-3 stretch-card transparent mb-3">
<div class="card card-light-blue">
<div class="card-body">
<p class="mb-4">RAM Usage</p>
<p class="fs-30 mb-2"><%= system.ram.used %> MB</p>
<p>of <%= system.ram.total %> MB</p>
</div>
</div>
</div>
<div class="col-md-3 stretch-card transparent mb-3">
<div class="card card-light-danger">
<div class="card-body">
<p class="mb-4">Platform</p>
<p class="fs-30 mb-2"><%= system.platform %></p>
<p><%= system.arch %> - <%= system.node %></p>
</div>
</div>
</div>
</div>
<div class="row">
<div class="col-md-12 grid-margin stretch-card">
<div class="card">
<div class="card-body">
<p class="card-title mb-0">Latest Requests</p>
<div class="table-responsive">
<table class="table table-striped table-borderless">
<thead>
<tr>
<th>Number</th>
<th>Path</th>
<th>Method</th>
<th>Time</th>
</tr>
</thead>
<tbody>
<% logs.forEach((log,i)=>{ %>
<tr>
<td><%= i+1 %></td>
<td><%= log.path %></td>
<td><span class="badge bg-<%= log.method=='GET'?'success':log.method=='POST'?'primary':log.method=='PUT'?'warning':log.method=='DELETE'?'danger':'secondary' %>"><%= log.method %></span></td>
<td><%= log.time %></td>
</tr>
<% }) %>
</tbody>
</table>
</div>
</div>
</div>
</div>
</div>
</div>
